#execute the files to print the log
CC = gcc

SRC = main.c holberton.c

OBJ = $(SRC:.c=.o)

NAME = holberton

RM = rm -f

CFLAGS = -Wall -Werror -Wextra -pedantic

.PHONY: clean fclean oclean all re

all: $(OBJ)  m.h
	$(CC) $(OBJ) -o $(NAME)

clean:
	$(RM) *~ $(NAME)

oclean:
	$(RM) $(OBJ)

fclean: clean oclean

re: oclean all